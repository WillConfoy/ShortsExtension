{
    "name": "Shorts Replacement Extension",
    "description": "Redirect shorts to normal youtube",
    "version": "1.0",
    "manifest_version": 3,
    "background": {"service_worker": "background.js"},
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "*://*.youtube.com/*"
    ]
  }